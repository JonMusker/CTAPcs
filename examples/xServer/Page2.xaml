      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:xServer"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="450"
      Title="Page2">

    <Grid>
        <Label Content="RPID" HorizontalAlignment="Left" Margin="62,161,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextRPID" HorizontalAlignment="Left" Height="23" Margin="150,161,0,0" TextWrapping="Wrap" Text="gebogebo.com" VerticalAlignment="Top" Width="120"/>

        <Label Content="Challenge" HorizontalAlignment="Left" Margin="62,207,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextChallenge" HorizontalAlignment="Left" Height="65" Margin="150,207,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="246"/>
        <Button x:Name="ButtonNext" Content="Next" HorizontalAlignment="Left" Margin="223,342,0,0" VerticalAlignment="Top" Width="173" Height="33" RenderTransformOrigin="0.566,-0.113" Click="ButtonNext_Click"/>
        <Label Content="Send to Client" HorizontalAlignment="Left" Margin="97,56,0,0" VerticalAlignment="Top"/>

    </Grid>
</Page>
